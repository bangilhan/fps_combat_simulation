{
  "version": 2,
  "cleanUrls": true,
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "routes": [
    { "src": "/$", "dest": "/index.html", "headers": { "cache-control": "no-cache" } },
    { "src": "/index.html", "dest": "/index.html", "headers": { "cache-control": "no-cache" } }
  ]
}


